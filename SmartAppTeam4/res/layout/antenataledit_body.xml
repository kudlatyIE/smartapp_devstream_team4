<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/antenataledit_layout_body"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:layout_below="@+id/serviceuseredit_header"
    android:layout_above="@+id/smart_layout_footer"
     >
    
    <LinearLayout
 		    android:layout_width="match_parent"
 		    android:layout_height="wrap_content"
 		    android:layout_margin="5dp"
 		    android:orientation="horizontal"
 		    >
 		    
 		    <EditText
 		    	android:id="@+id/antenataledit_edit_edd"
 		    	android:layout_width="0dp"
 		    	android:layout_height="wrap_content"		    	    
 		    	android:gravity="start"
 		    	android:layout_weight="1"
 		    	android:inputType="text"
 		    	/>
 		    	
 		    <TextView 
 		    	android:id="@+id/antenataledit_textview_edd"
 		    	android:layout_width="wrap_content"
 		    	android:layout_height="wrap_content" 		    	    
 		    	android:hint="@string/textview_edd" 		    	    
 		    	/>
 		    	
 		</LinearLayout>		    	
 		    
 		<LinearLayout
 		    android:layout_width="match_parent"
 		    android:layout_height="wrap_content"
 		    android:layout_margin="5dp"
 		    android:orientation="horizontal"
 		    >
 		    	
 		   <EditText
 		   		android:id="@+id/antenataledit_edit_gestation"
 		    	android:layout_width="0dp"
 		    	android:layout_height="wrap_content"		    	    
 		    	android:gravity="start"
 		    	android:layout_weight="1"
 		    	android:inputType="text"
 		    	/>
 		    	
 		    <TextView 
 		    	android:id="@+id/antenataledit_textView_gestatiom"
 		    	android:layout_width="wrap_content"
 		    	android:layout_height="wrap_content"
 		    	android:hint="@string/textview_gestation" 		    	    
 		    	/>
 		    		    
 		</LinearLayout>  
 		
 	 	<LinearLayout
 		    android:layout_width="match_parent"
 		    android:layout_height="wrap_content"
 		    android:layout_margin="5dp"
 		    android:orientation="horizontal"
 		    >
 		    	
 		    <Spinner 
 		        android:id="@+id/antenataledit_spinner_blood"
 		        android:layout_width="wrap_content"
 		    	android:layout_height="wrap_content"
 		        android:prompt="@string/spinner_blood_title"
 		        />
 		    
 		    <TextView 
 		    	android:id="@+id/antenataledit_textView_blood"
 		    	android:layout_width="0dp"
 		    	android:layout_height="wrap_content"
 		    	android:layout_weight="1"
 		        android:gravity="end"
 		    	android:text="bloodType"	    	    
 		    	/>
 		    
 		    		    
 		</LinearLayout> 	
 		
 	 	 <LinearLayout
 		    android:layout_width="match_parent"
 		    android:layout_height="wrap_content"
 		    android:layout_margin="5dp"
 		    android:orientation="horizontal"
 		    >
 		    	
 		    <Spinner
 		    	android:id="@+id/antenataledit_spinner_rhesus"
 		    	android:layout_width="wrap_content"
 		    	android:layout_height="wrap_content"
 		    	android:prompt="@string/spinner_rhesus_title"	    	    
 		    	/>
 		    	
 		    <TextView 
 		    	android:id="@+id/antenataledit_textView_rhesus"
 		    	android:layout_width="0dp"
 		    	android:layout_height="wrap_content"
 		    	android:layout_weight="1"
 		    	android:gravity="end"
 		    	android:hint="@string/textview_rhesus" 		    	    
 		    	/>
 		    		    
 		</LinearLayout> 
 		
 	 	<LinearLayout
 		    android:layout_width="match_parent"
 		    android:layout_height="wrap_content"
 		    android:layout_margin="5dp"
 		    android:orientation="horizontal"
 		    >
 		    	
 		    <EditText
 		    	android:id="@+id/antenataledit_edit_parity"
 		    	android:layout_width="0dp"
 		    	android:layout_height="wrap_content"	    	    
 		    	android:gravity="start"
 		    	android:layout_weight="1"
 		    	android:inputType="text"		    	    
 		    	/>
 		    	
 		    <Button
 		    	android:id="@+id/antenataledit_btn_parity"
 		    	android:layout_width="wrap_content"
 		    	android:layout_height="wrap_content"			    	    
 		    	android:hint="@string/btn_parity" 		    	    
 		    	/>
 		    		    
 		</LinearLayout> 
 		
 	 	<LinearLayout
 		    android:layout_width="match_parent"
 		    android:layout_height="wrap_content"
 		    android:layout_margin="5dp"
 		    android:orientation="horizontal"
 		    >
 		    	
 		    <EditText
 		    	android:id="@+id/antenataledit_edit_obstretic_history"
 		    	android:layout_width="0dp"
 		    	android:layout_height="wrap_content"	    	    
 		    	android:gravity="start"
 		    	android:layout_weight="1"
 		    	android:inputType="text"	    	    
 		    	/>
 		    	
 		    <Button
 		   		android:id="@+id/antenataledit_btn_obstretic_history"
 		    	android:layout_width="wrap_content"
 		    	android:layout_height="wrap_content"	    	    
 		    	android:hint="@string/btn_obstretic_history" 		    	    
 		    	/>
 		    		    
 		</LinearLayout>
    

</LinearLayout>
